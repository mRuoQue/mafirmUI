{
  "name": "smallma-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "vue3 ui library",
  "main": "./dist/umd/smallmaUI.umd.js",
  "module": "./dist/es/smallmaUI.es.js",
  "types": "./dsit/types/smallmaUI.d.ts",
  "files": [
    "dsit"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.es.js",
      "require": "./dist/umd/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p build:es build:umd",
    "build:only": "vite build",
    "build:es": "vite build --config vite.config.es.ts",
    "build:umd": "vite build --config vite.config.umd.ts",
    "test": "vitest",
    "postinstall": "patch-package",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@smallma-ui/components": "workspace:*",
    "@smallma-ui/hooks": "workspace:*",
    "@smallma-ui/utils": "workspace:*",
    "vue": "^3.5.26"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^25.0.5",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitepress-demo-preview/component": "^2.6.1",
    "@vitepress-demo-preview/plugin": "^1.4.1",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^25.0.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.1",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^7.0.2",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.16"
  }
}
